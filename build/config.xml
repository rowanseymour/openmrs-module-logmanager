<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//OpenMRS//DTD OpenMRS Config 1.0//EN" "./config-1.0.dtd">
<module configVersion="1.0">
	<id>logmanager</id>
	<name>Log Manager</name>
	<version>1.0</version>
	<package>org.openmrs.module.logmanager</package>
	<author>Rowan Seymour</author>
	<description>Management of logging for developers</description>
	<activator>org.openmrs.module.logmanager.ModuleActivator</activator>
	<updateURL>https://dev.openmrs.org/modules/download/logmanager/update.rdf</updateURL>
	<require_version>1.4.3</require_version>
	
	<extension>
		<point>org.openmrs.admin.maintenance.localHeader</point>
		<class>org.openmrs.module.logmanager.extension.html.MaintenanceLinksExt</class>
	</extension>
	
	<privilege>
		<name>Manage Server Log</name>
		<description>Able to manage and configure server logging</description>
	</privilege>
	
	<messages>
		<lang>en</lang>
		<file>messages.properties</file>
	</messages>
</module>

